{
  "manifest_version": 2,
  "name": "Spaces API Example",
  "version": "1.0",
  "description": "An extension to create a space using the Spaces API.",
  "applications": {
    "gecko": {
      "id": "spaces-api-example@example.com",
      "strict_min_version": "102.0"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "skin/images/icon.png"
  },
  "web_accessible_resources": [
    "content/anais.html"
  ]
}
